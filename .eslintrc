{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "root":true,
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "block-scoped-var":2,
    "complexity":[1,7],
    "consistent-return":2,
    "curly":2,
    "max-depth":[1,3],
    "max-params":[1,4],
    "max-statements":[1,25],
    "dot-notation":2,
    "eqeqeq":2,
    "guard-for-in":2,
    "quotes":[1,"single"],
    "no-alert":1,
    "no-caller":2,
    "no-console":0,
    "no-div-regex":2,
    "no-else-return":2,
    "no-empty-pattern":2,
    "no-eq-null":2,
    "no-eval":2,
    "no-extra-parens":2,
    "no-labels":2,
    "no-lone-blocks":2,
    "no-loop-func":2,
    "no-multi-spaces":2,
    "no-negated-condition":2,
    "no-param-reassign":2,
    "no-return-assign":2,
    "no-self-compare":2,
    "no-sequences":2,
    "no-spaced-func":2,
    "no-trailing-spaces":2,
    "no-undef-init":2,
    "no-unexpected-multiline":2,
    "no-unneeded-ternary":2,
    "no-unused-expressions":2,
    "no-use-before-define":1,
    "no-useless-concat":2,
    "one-var":[2, "never"],
    "no-var":2,
    "prefer-const":1,
    "semi":1,
    "block-spacing":2,
    "brace-style":[2,"1tbs",{ "allowSingleLine": true }],
    "no-lonely-if":2,
    "no-with":2,
    "computed-property-spacing":2,
    "keyword-spacing":2,
    "space-before-blocks":2,
    "space-infix-ops":2,
    "space-unary-ops":2
  }
}
